{
  "name": "pwa-template",
  "version": "1.0.0",
  "description": "A lightweight create-react-app alternative for typical React project (React.js, Redux, Redux Sagas, Reselect, Ramda.js and etc.)",
  "repository": {
    "type": "git",
    "url": "https://github.com/AdeonMaster/pwa-template.git"
  },
  "homepage": "https://adeonmaster.github.io/pwa-template/",
  "main": "index.js",
  "scripts": {
    "build:watch": "webpack -w --config config/webpack/dev.config.js",
    "build:dev": "webpack --config config/webpack/dev.config.js",
    "build": "webpack --config config/webpack/prod.config.js",
    "server": "node server/http.js",
    "server:ssl": "node server/https.js",
    "eslint": "eslint ./src",
    "eslint:fix": "eslint --fix ./src",
    "prettier": "prettier --write src/**/*.js",
    "jest": "jest --config config/jest/jest.config.json",
    "jest:watch": "jest --config config/jest/jest.config.json --watch",
    "jest:coverage": "jest --config config/jest/jest.config.json --coverage",
    "analyze": "source-map-explorer 'static/js/*.js'",
    "deploy": "gh-pages -d dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint",
      "pre-push": "npm run eslint"
    }
  },
  "author": "AdeonMaster",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-export-default-from": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@testing-library/react": "^10.2.1",
    "@testing-library/react-hooks": "^3.3.0",
    "autoprefixer": "^9.8.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-root-import": "^6.5.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.4",
    "copy-webpack-plugin": "^6.0.2",
    "cors": "^2.8.5",
    "css-loader": "^3.5.3",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-sonarjs": "^0.5.0",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "gh-pages": "^3.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "2.0.5",
    "react-test-renderer": "^16.13.1",
    "redux-saga-test-plan": "^4.0.0-rc.3",
    "sass-loader": "^8.0.2",
    "source-map-explorer": "^2.4.2",
    "spdy": "^4.0.2",
    "terser-webpack-plugin": "^3.0.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "wrapper-webpack-plugin": "^2.1.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.10",
    "@welldone-software/why-did-you-render": "^4.2.5",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "core-js": "^3.6.5",
    "offline-plugin": "^5.0.7",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-popper": "^2.2.3",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.4.1",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "^0.13.5",
    "reselect": "^4.0.0",
    "shortid": "^2.2.15"
  }
}
